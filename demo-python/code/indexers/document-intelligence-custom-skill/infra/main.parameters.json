{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "searchServiceLocation": {
      "value": "${AZURE_SEARCH_SERVICE_LOCATION}"
    },
    "searchServiceName": {
      "value": "${AZURE_SEARCH_SERVICE}"
    },
    "searchServiceResourceGroupName": {
      "value": "${AZURE_SEARCH_SERVICE_RESOURCE_GROUP}"
    },
    "searchServiceSkuName": {
      "value": "${AZURE_SEARCH_SERVICE_SKU=standard}"
    },
    "semanticSearchSkuName": {
      "value": "${AZURE_SEARCH_SEMANTIC_SEARCH_SKU=free}"
    },
    "storageLocation": {
      "value": "${AZURE_STORAGE_ACCOUNT_LOCATION}"
    },
    "storageResourceGroupName": {
      "value": "${AZURE_STORAGE_ACCOUNT_RESOURCE_GROUP}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT}"
    },
    "appServicePlanName": {
      "value": "${AZURE_APP_SERVICE_PLAN}"
    },
    "apiServiceName": {
      "value": "${AZURE_API_SERVICE}"
    },
    "apiServiceLocation": {
      "value": "${AZURE_API_SERVICE_LOCATION}"
    },
    "apiServiceResourceGroupName": {
      "value": "${AZURE_API_SERVICE_RESOURCE_GROUP}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOG_ANALYTICS}"
    },
    "applicationInsightsName": {
      "value": "${AZURE_APPINSIGHTS}"
    },
    "sentenceTransformersTextEmbeddingModel": {
      "value": "${SENTENCE_TRANSFORMERS_TEXT_EMBEDDING_MODEL}"
    }
  }
}
