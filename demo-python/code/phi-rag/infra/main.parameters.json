{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "openAiAccountName": {
      "value": "${AZURE_OPENAI_ACCOUNT}"
    },
    "openAiSkuName": {
      "value": "${AZURE_OPENAI_SKU=S0}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION}"
    },
    "openAiResourceGroupName": {
      "value": "${AZURE_OPENAI_RESOURCE_GROUP}"
    },
    "openAiEmbeddingModelName": {
      "value": "${AZURE_OPENAI_EMB_MODEL_NAME=text-embedding-3-large}"
    },
    "openAiEmbeddingModelDimensions": {
      "value": "${AZURE_OPENAI_EMB_MODEL_DIMENSIONS=3072}"
    },
    "openAiEmbeddingDeploymentName": {
      "value": "${AZURE_OPENAI_EMB_DEPLOYMENT=text-embedding-3-large}"
    },
    "openAiEmbeddingDeploymentCapacity": {
      "value": "${AZURE_OPENAI_EMB_DEPLOYMENT_CAPACITY=100}"
    },
    "storageLocation": {
      "value": "${AZURE_STORAGE_ACCOUNT_LOCATION}"
    },
    "storageResourceGroupName": {
      "value": "${AZURE_STORAGE_ACCOUNT_RESOURCE_GROUP}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT}"
    },
    "storageSkuName": {
      "value": "${AZURE_STORAGE_SKU=Standard_LRS}"
    },
    "searchServiceLocation": {
      "value": "${AZURE_SEARCH_SERVICE_LOCATION}"
    },
    "searchServiceResourceGroupName": {
      "value": "${AZURE_SEARCH_SERVICE_RESOURCE_GROUP}"
    },
    "searchServiceName": {
      "value": "${AZURE_SEARCH_SERVICE}"
    },
    "searchServiceSkuName": {
      "value": "${AZURE_SEARCH_SERVICE_SKU=standard}"
    },
    "searchServiceSemanticRanker": {
      "value": "${AZURE_SEARCH_SERVICE_SEMANTIC_RANKER=free}"
    },
    "searchIndexName": {
      "value": "${AZURE_SEARCH_INDEX=phirag}"
    }
  }
}
