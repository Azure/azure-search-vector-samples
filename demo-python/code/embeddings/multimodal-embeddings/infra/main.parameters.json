{
  "$schema": "https://schema.management.azure.com/schemas/2019-04-01/deploymentParameters.json#",
  "contentVersion": "1.0.0.0",
  "parameters": {
    "environmentName": {
      "value": "${AZURE_ENV_NAME}"
    },
    "location": {
      "value": "${AZURE_LOCATION}"
    },
    "resourceGroupName": {
      "value": "${AZURE_RESOURCE_GROUP}"
    },
    "principalId": {
      "value": "${AZURE_PRINCIPAL_ID}"
    },
    "aiServicesAccountName": {
      "value": "${AZURE_AI_SERVICES_ACCOUNT}"
    },
    "aiServicesSkuName": {
      "value": "${AZURE_AI_SERVICES_SKU=S0}"
    },
    "aiServicesLocation": {
      "value": "${AZURE_AI_SERVICES_LOCATION=eastus}"
    },
    "aiServicesResourceGroupName": {
      "value": "${AZURE_AI_SERVICES_RESOURCE_GROUP}"
    },
    "storageLocation": {
      "value": "${AZURE_STORAGE_ACCOUNT_LOCATION}"
    },
    "storageResourceGroupName": {
      "value": "${AZURE_STORAGE_ACCOUNT_RESOURCE_GROUP}"
    },
    "storageAccountName": {
      "value": "${AZURE_STORAGE_ACCOUNT}"
    },
    "storageSkuName": {
      "value": "${AZURE_STORAGE_SKU=Standard_LRS}"
    },
    "searchServiceLocation": {
      "value": "${AZURE_SEARCH_SERVICE_LOCATION}"
    },
    "searchServiceResourceGroupName": {
      "value": "${AZURE_SEARCH_SERVICE_RESOURCE_GROUP}"
    },
    "searchServiceName": {
      "value": "${AZURE_SEARCH_SERVICE}"
    },
    "searchServiceSkuName": {
      "value": "${AZURE_SEARCH_SERVICE_SKU=standard}"
    },
    "searchServiceSemanticRanker": {
      "value": "${AZURE_SEARCH_SERVICE_SEMANTIC_RANKER=free}"
    },
    "searchIndexName": {
      "value": "${AZURE_SEARCH_INDEX=multimodalembeddings}"
    },
    "openAiAccountName": {
      "value": "${AZURE_OPENAI_ACCCOUNT}"
    },
    "openAiLocation": {
      "value": "${AZURE_OPENAI_LOCATION}"
    },
    "openAiResourceGroupName": {
      "value": "${AZURE_OPENAI_RESOURCE_GROUP}"
    },
    "openAiChatGptDeploymentName": {
      "value": "${AZURE_OPENAI_CHATGPT_DEPLOYMENT=gpt-4o}"
    },
    "openAiChatGptModelName": {
      "value": "${AZURE_OPENAI_CHATGPT_MODEL=gpt-4o}"
    },
    "openAiChatGptModelVersion": {
      "value": "${AZURE_OPENAI_CHATGPT_MODEL_VERSION=2024-05-13"
    },
    "openAiChatGptDeploymentCapacity": {
      "value": "${AZURE_OPENAI_CHATGPT_DEPLOYMENT_CAPACITY=10}"
    },
    "apiServiceName": {
      "value": "${AZURE_API_SERVICE}"
    },
    "apiServiceResourceGroupName": {
      "value": "${AZURE_API_SERVICE_RESOURCE_GROUP}"
    },
    "appServicePlanName": {
      "value": "${AZURE_APP_SERVICE_PLAN}"
    },
    "logAnalyticsName": {
      "value": "${AZURE_LOG_ANALYTICS}"
    },
    "appInsightsName": {
      "value": "${AZURE_APPINSIGHTS}"
    }
  }
}
